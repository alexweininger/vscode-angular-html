{
  "name": "vscode-angular-html",
  "displayName": "vscode-angular-html",
  "description": "Syntax highlighting for angular HTML Template files",
  "version": "0.0.34",
  "publisher": "ghaschel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghaschel/vscode-angular-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/ghaschel/vscode-angular-syntax/issues"
  },
  "scripts": {
    "watch": "gulp watch",
    "compile-json": "gulp compile-json"
  },
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "html",
        "aliases": [
          "HTML (Angular template)"
        ],
        "extensions": [
          ".component.html"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "html",
        "scopeName": "text.html.html",
        "path": "./syntaxes/angular-html.tmLanguage.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "icon": "assets/angular-html.png",
  "devDependencies": {
    "gulp": "^4.0.0",
    "gulp-json5-to-json": "^0.2.1",
    "gulp-merge-json": "^1.3.1",
    "json-beautify": "^1.0.1"
  }
}
