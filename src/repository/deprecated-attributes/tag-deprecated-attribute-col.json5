{
  repository: {
    'tag-deprecated-attribute-col': {
      name: 'entity.other.attribute-name',
      begin: '(?<=<col)',
      end: '(?=[<>])',
      patterns: [
        {
          name: 'invalid.deprecated.attribute-name.col',
          match: '[\\s\\n\\r\\t]*(\\b(char|charoff|valign|width)\\b)(=)?',
          captures: {
            '2': {
              name: 'invalid.deprecated.attribute-name.col'
            },
            '3': {
              name: 'constant.character.entity.html'
            }
          }
        },
        {
          include: '#tag-stuff'
        }
      ]
    }
  }
}
