{
  repository: {
    'tag-deprecated-attribute-link': {
      begin: '(?<=<link)',
      end: '(?=[\\/>])',
      patterns: [
        {
          name: 'invalid.deprecated.attribute-name.link',
          match: '[\\s\\n\\r\\t]*(\\b(charset|rev|target)\\b)(=)?',
          captures: {
            '2': {
              name: 'invalid.deprecated.attribute-name.link'
            },
            '3': {
              name: 'constant.character.entity.html'
            }
          }
        },
        {
          include: '#tag-stuff'
        }
      ]
    }
  }
}
