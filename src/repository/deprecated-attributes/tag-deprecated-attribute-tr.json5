{
  repository: {
    "tag-deprecated-attribute-tr": {
      name: "entity.other.attribute-name",
      begin: "(?<=<tr)",
      end: "(?=[<>])",
      patterns: [
        {
          name: "invalid.deprecated.attribute-name.tr",
          match: "[\\s\\n\\r\\t]*(\\b(align|bgcolor|char|charoff|valign)\\b)(?==)"
