{
  repository: {
    'tag-deprecated-attribute-thead': {
      begin: '(?<=<thead)',
      end: '(?=[>])',
      patterns: [
        {
          name: 'invalid.deprecated.attribute-name.thead',
          match: '[\\s\\n\\r\\t]*(\\b(align|char|charoff|valign)\\b)(=)?',
          captures: {
            '2': {
              name: 'invalid.deprecated.attribute-name.thead'
            },
            '3': {
              name: 'constant.character.entity.html'
            }
          }
        },
        {
          include: '#tag-stuff'
        }
      ]
    }
  }
}
