{
  repository: {
    'tag-deprecated-attribute-param': {
      begin: '(?<=<param)',
      end: '(?=[/>])',
      patterns: [
        {
          name: 'invalid.deprecated.attribute-name.param',
          match: '[\\s\\n\\r\\t]*(\\b(type|valuetype)\\b)(=)?',
          captures: {
            '2': {
              name: 'invalid.deprecated.attribute-name.param'
            },
            '3': {
              name: 'constant.character.entity.html'
            }
          }
        },
        {
          include: '#tag-stuff'
        }
      ]
    }
  }
}
