{
  repository: {
    'tag-deprecated-attribute-head': {
      begin: '(?<=<head)',
      end: '(?=[>])',
      patterns: [
        {
          name: 'invalid.deprecated.attribute-name.head',
          match: '[\\s\\n\\r\\t]*(\\b(profile)\\b)(=)?',
          captures: {
            '2': {
              name: 'invalid.deprecated.attribute-name.head'
            },
            '3': {
              name: 'constant.character.entity.html'
            }
          }
        },
        {
          include: '#tag-stuff'
        }
      ]
    }
  }
}
