{
  repository: {
    "method-declaration-name": {
			"begin": "(?=(([_$[:alpha:]][_$[:alnum:]]*)|(\\'[^']*\\')|(\\\"[^\"]*\\\")|(\\[([^\\[\\]]|\\[[^\\[\\]]+\\])+\\]))\\s*(\\??)\\s*[\\(\\<])",
			"end": "(?=\\(|\\<)",
			"patterns": [
        {
          "include": "#ts-string"
        },
				{
					"include": "#array-literal"
				},
				{
					"name": "entity.name.function.ts",
					"match": "[_$[:alpha:]][_$[:alnum:]]*"
				},
				{
					"name": "keyword.operator.optional.ts",
					"match": "\\?"
				}
			]
		}
  }
}