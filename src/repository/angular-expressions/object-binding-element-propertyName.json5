{
  repository: {
    "object-binding-element-propertyName": {
			"begin": "(?=(([_$[:alpha:]][_$[:alnum:]]*)|(\\'[^']*\\')|(\\\"[^\"]*\\\")|(\\[([^\\[\\]]|\\[[^\\[\\]]+\\])+\\]))\\s*(:))",
			"end": "(:)",
			"endCaptures": {
				"0": {
					"name": "punctuation.destructuring.ts"
				}
			},
			"patterns": [
				{
          "include": "#ts-string"
        },
				{
					"include": "#array-literal"
				},
				{
					"name": "variable.object.property.ts",
					"match": "([_$[:alpha:]][_$[:alnum:]]*)"
				}
			]
		}
  }
}