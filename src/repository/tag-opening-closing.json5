{
  repository: {
    'tag-opening-closing': {
      name: 'entity.name.tag',
      captures: {
        '1': {
          name: 'punctuation.definition.tag.begin.html'
        },
        '2': {
          name: 'entity.name.tag.end.html'
        },
        '3': {
          name: 'punctuation.definition.tag.end.html'
        }
      },
      match: '(<\\/?)((?!script)[a-zA-Z0-9\\-]++*)|(\\/?>)',
      patterns: [
        {
          include: '#tag-stuff'
        }
      ]
    }
  }
}
